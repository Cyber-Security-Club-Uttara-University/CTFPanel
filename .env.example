# CTFd Docker Environment Variables
# Copy this file to .env and modify the values as needed

# Database Configuration
MYSQL_ROOT_PASSWORD=pranxten
MYSQL_USER=ctfd
MYSQL_PASSWORD=pranxten
MYSQL_DATABASE=ctfd

# CTFd Configuration
SECRET_KEY=your_secret_key_here_change_me
WORKERS=1
UPLOAD_FOLDER=/var/uploads
LOG_FOLDER=/var/log/CTFd

# Redis Configuration
REDIS_URL=redis://cache:6379

# Database URL for CTFd
DATABASE_URL=mysql+pymysql://ctfd:pranxten@db/ctfd

# Reverse proxy settings
REVERSE_PROXY=true

# Logging
ACCESS_LOG=-
ERROR_LOG=-
